<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>campsServlet</servlet-name>
    <servlet-class>edu.washington.cs.cse403.camps.server.CampsServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>campsServlet</servlet-name>
    <url-pattern>/servlet/CampsServlet</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Camps.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>Upload</servlet-name>
    <servlet-class>edu.washington.cs.cse403.camps.server.Upload</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>

</web-app>
